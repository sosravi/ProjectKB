# Production Environment Variables Template
# Copy this file to .env.production and fill in your actual values

# Environment
NODE_ENV=production

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1

# S3 Configuration
S3_BUCKET=your_projectkb_uploads_bucket_name_prod
TRANSCRIBE_BUCKET=your_projectkb_transcribe_bucket_name_prod

# Cognito Configuration
COGNITO_USER_POOL_ID=your_cognito_user_pool_id_prod
COGNITO_CLIENT_ID=your_cognito_client_id_prod

# API Configuration
API_URL=https://your_api_gateway_url.execute-api.us-east-1.amazonaws.com/prod

# AI Services Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# Amplify Configuration
AMPLIFY_APP_ID=your_amplify_app_id_prod
AMPLIFY_BRANCH_NAME=main

# Domain Configuration
DOMAIN_NAME=your_custom_domain.com
HOSTED_ZONE_ID=your_route53_hosted_zone_id
SSL_CERTIFICATE_ARN=your_ssl_certificate_arn

# Database Configuration
DYNAMODB_TABLE_PREFIX=ProjectKB-Prod

# Security Configuration
JWT_SECRET=your_production_jwt_secret_key_here

# Monitoring Configuration
CLOUDWATCH_LOG_GROUP=/aws/lambda/projectkb-prod

# Notification Configuration
SLACK_WEBHOOK_URL=your_slack_webhook_url
SNYK_TOKEN=your_snyk_token

# Frontend Environment Variables (for React build)
REACT_APP_API_URL=https://your_api_gateway_url.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_COGNITO_USER_POOL_ID=your_cognito_user_pool_id_prod
REACT_APP_COGNITO_CLIENT_ID=your_cognito_client_id_prod
REACT_APP_S3_BUCKET=your_projectkb_uploads_bucket_name_prod
REACT_APP_BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
REACT_APP_DOMAIN_NAME=your_custom_domain.com

# Production Configuration
DEBUG=false
LOG_LEVEL=info
ENABLE_MONITORING=true
ENABLE_ANALYTICS=true


